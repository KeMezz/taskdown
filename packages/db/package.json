  "dependencies": {
    "drizzle-orm": "^0.38.3"
  },
  "devDependencies": {
    "drizzle-kit": "^0.30.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.38.0"
  }
